{
  "name": "flexberry-launcher",
  "version": "0.4.4",
  "description": "Lightweight and useful Minecraft launcher",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "pack-linux": "npx electron-packager . flexberry-launcher --overwrite --platform=linux --arch=x64 --icon=src/assets/logo.ico --prune=true",
    "pack": "npx electron-packager . flexberry-launcher --overwrite --icon=src/assets/logo.ico --prune=true"
  },
  "repository": "https://github.com/kuzeeeyk/flexberry-launcher",
  "keywords": [
    "Minecraft",
    "launcher",
    "flexberry"
  ],
  "author": "kuzeeeyk",
  "license": "MIT",
  "dependencies": {
    "ejs-electron": "^2.1.1",
    "lowdb": "^1.0.0",
    "minecraft-launcher-core": "^3.16.4"
  },
  "devDependencies": {
    "electron": "^13.1.8",
    "electron-packager": "^15.3.0"
  }
}